clc;clear;
%还没正确
A=[0 1;1 0];
B=[1;1];
F=[-1,-1];
tau=0.1;
alpha=0;
E=exp(-2*alpha*tau);

setlmis([]);
P=lmivar(1,[2,1]);
P2=lmivar(1,[2,1]);
P3=lmivar(1,[2,1]);
R=lmivar(1,[2,1]);
S=lmivar(1,[2,1]);

base1=newlmi;
lmiterm([-base1 1 1 P],1,1);

base2=newlmi;
lmiterm([-base2 1 1 R],1,1);

base3=newlmi;
lmiterm([-base3 1 1 S],1,1);

a1=newlmi;
lmiterm([a1 1 1 R],E,1);
lmiterm([a1 1 1 S],1,1);
lmiterm([a1 1 1 P2],1,A,'s');
lmiterm([a1 1 1 P],2*alpha,1);


lmiterm([a1 1 2 P2],1,B*F);
lmiterm([a1 1 2 R],E,1);


lmiterm([a1 1 3 P2],-1,1);
lmiterm([a1 1 3 P],1,1);
lmiterm([a1 1 3 P3],A',1);

lmiterm([a1 2 2 S],-1,E);
lmiterm([a1 2 2 R],1,E);

lmiterm([a1 2 3 P3],F'*B',1);

lmiterm([a1 3 3 P3],-1,1,'s');
lmiterm([a1 3 3 R],tau,tau);

lmisys=getlmis;
options=[0,0,0,0,0];
[tmin,xfeas]=feasp(lmisys,options,-1);